<table id="table-presupuesto" class="table table-condensed">
    <tbody>
        {# start: INGRESOS #}
        <tr>
            <td colspan="1"></td>
            <td class="tr-titulo">INGRESOS</td>
            <td class="col-lg-4 text-center" colspan="4">Valor Unitario (VU) = C1*T1 + C2*T2</td>
            {# <td class="col-lg-1"></td> #}
            {# <td class="col-lg-1"></td> #}
            {# <td class="col-lg-1"></td> #}
            <td class="col-lg-1"></td>
        </tr>
        <tr class="tr-super-title">
            <td class="td-descripcion">Item</td>
            <td class="td-descripcion">Descripción</td>
            <td class="td-bordered td-bordered-no-right">C1</td>
            <td class="td-bordered td-bordered-no-right">T1</td>
            <td class="td-bordered td-bordered-no-right">C2</td>
            <td class="td-bordered">T2</td>
            <td></td>
        </tr>
        {% for item in arrIngresos %}
            <tr class="item-ingreso" data-class="item-ingreso">
                <td class="text-center">
                    {{ item.item }}
                    <input type='hidden' class='numero_item' name='ingreso_item[{{ item.item }}]' value='{{ item.item }}'>
                </td>
                <td><input type='text' class='form-control ingreso_descripcion input-sm' name='ingreso_descripcion[{{ item.item }}]' value='{{ item.Descripcion }}' readonly='readonly'></td>
                <td><input type='text' class='form-control ingreso_c1 input-sm input-monto' name='ingreso_c1[{{ item.item }}]' value='{{ item.C1 | number_format(0, '.', '.') }}'></td>
                <td><input type='text' class='form-control ingreso_t1 input-sm input-monto' name='ingreso_t1[{{ item.item }}]' value='{{ item.T1 | number_format(0, '.', '.') }}'></td>
                <td><input type='text' class='form-control ingreso_c2 input-sm input-monto' name='ingreso_c2[{{ item.item }}]' value='{{ item.C2 | number_format(0, '.', '.') }}'></td>
                <td><input type='text' class='form-control ingreso_t2 input-sm input-monto' name='ingreso_t2[{{ item.item }}]' value='{{ item.T2 | number_format(0, '.', '.') }}'></td>
                <td>
                    <div class='btn-group pull-right'>
                        <a href='javascript:' class='btn-eliminar-item-antiguo btn btn-danger btn-xs' title='Eliminar'>
                            Eliminar
                        </a>
                    </div>
                </td>
            </tr>
        {% endfor %}
        <tr class="item-ingreso"></tr>
        <tr>
            <td></td>
            <td>
                <select id="select-item-ingreso" class='form-control'>
                    <option value="null">Seleccionar Item</option>
                    {% for codigo, item in arrItems.ingresos %}
                        <option value="{{ codigo }}">{{ item }}</option>
                    {% endfor %}
                </select>
            </td>
            <td>
                <a href='javascript:' class='btn-agregar-item-ingreso btn btn-success btn-sm btn-block' title='Agregar Item'>
                    Agregar Item Ingreso
                </a>
            </td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
        </tr>
        {# end: INGRESOS #}
        {# start: COSTOS FIJOS #}
        <tr>
            <td colspan="1"></td>
            <td class="tr-titulo">COSTOS FIJOS</td>
        </tr>
        <tr class="tr-super-title">
            <td class="td-descripcion">Item</td>
            <td class="td-descripcion">Descripción</td>
            <td class="td-bordered td-bordered-no-right">C1</td>
            <td class="td-bordered td-bordered-no-right">T1</td>
            <td class="td-bordered td-bordered-no-right">C2</td>
            <td class="td-bordered">T2</td>
            <td></td>
        </tr>
        {% for item in arrCostosFijos %}
